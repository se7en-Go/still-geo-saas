# OCR 配置
OCR_API_KEY=

# 文件上传配置
UPLOAD_DIR=/app/uploads
MAX_FILE_SIZE=10485760

# 其他配置
ALLOW_USER_REGISTRATION=false
CORS_ORIGIN=*