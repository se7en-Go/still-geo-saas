
# PostgreSQL Database Configuration
DB_USER=your_database_user
DB_HOST=your_database_host
DB_DATABASE=geo_saas
DB_PASSWORD=your_database_password
DB_PORT=5432

# Server Configuration
PORT=3001
JWT_SECRET=your_jwt_secret
ALLOW_USER_REGISTRATION=false

# OCR (optional)
OCR_ENABLED=false
OCR_PROVIDER=deepseek
OCR_BASE_URL=https://api.siliconflow.cn
OCR_API_KEY=your_ocr_api_key
OCR_MODEL=deepseek-ai/DeepSeek-OCR
OCR_ENDPOINT=/v1/chat/completions
OCR_TIMEOUT_MS=90000

# Keyword cache tuning
KEYWORD_CACHE_TTL_MS=60000
KEYWORD_CACHE_MAX_ENTRIES=200
